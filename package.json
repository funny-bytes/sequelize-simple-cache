{
  "name": "sequelize-simple-cache",
  "version": "1.0.2",
  "description": "A simple, transparent, client-side, in-memory cache for Sequelize",
  "main": "src/SequelizeSimpleCache.js",
  "author": "Frank Thelen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/frankthelen/sequelize-simple-cache"
  },
  "keywords": [
    "Sequelize",
    "transparent",
    "cache",
    "client-side",
    "in-memory"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path ./.eslintignore",
    "test": "NODE_ENV=test mocha --recursive test",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive test",
    "coveralls": "npm run cover && nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "peerDependencies": {
    "sequelize": ">=4.x.x"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-should-promised": "^2.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sequelize": "^5.15.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "md5": "^2.2.1"
  }
}
